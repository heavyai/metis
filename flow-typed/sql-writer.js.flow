
declare type SQLWriter = {|
  writeSQL: (state: DataState) => string,
  registerParser: (typeDef: any, (transform: any) => SQL) => void,
  parseExpression: (expression: Expression) => string,
  parseTransform: (transform: Transform) => SQL
|}

declare type SQL = {|
  select: Array<string>,
  from: string,
  where: Array<string>,
  groupby: Array<string>,
  having: Array<string>,
  orderby: Array<string>,
  limit: string,
  offset: string
|};
